{
	"arrowParens": "always",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"endOfLine": "auto",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"jsxSingleQuote": false,
	"printWidth": 80,
	"proseWrap": "always",
	"quoteProps": "consistent",
	"requirePragma": false,
	"semi": true,
	"singleQuote": false,
	"tabWidth": 4,
	"useTabs": true,
	"trailingComma": "all",
	"importOrder": [
		"@acme/sass/global\\.scss",
		"(\\.scss|\\.less|\\.css|\\.sass)$",
		"(\\.jpg|\\.jpeg|\\.svg|\\.png|\\.webp)$",
		"^(^antd|^rsuite|^@emotion|^emotion|theme-ui|^semantic-ui|^@rebass|^@chakra-ui|bootstrap|tailwind|bulma|mui|^@blueprintjs|reactstrap|^@mantine)",
		"^(^react$|^@react|^react|^recoil|redux|vite)",
		"@acme.*",
		"<THIRD_PARTY_MODULES>",
		"^[./]",
		".*types\\.ts"
	],
	"importOrderGroupNamespaceSpecifiers": true,
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
